! -- Urxvt -- !
urxvt*termName: rxvt-unicode-256color
urxvt*.scrollBar_right: false
urxvt*scrollBar: false
urxvt*borderLess: false

! -- Font settings -- !
xft.autohint: 0
xft.lcdfilter: lcddefault
xft.hintstyle: hintslight
xft.hinting: 1
xft.antialias: 1
xft.dpi: 96
xft.rgba: rgb

! -- Fonts -- !
!urxvt.font:xft:Inconsolata:medium:size=12
urxvt.font:xft:Source Code Pro for Powerline:RegularForPowerline:12
urxvt.boldFont:xft:Source Code Pro for Powerline:BoldForPowerline:12

! -- Colours -- !
!#include ".Xcolors/Molokai"
!#include ".Xcolors/Solarized"
!#include ".Xcolors/Technicolor_I"
!#include ".Xcolors/Technicolor_II"
!#include ".Xcolors/Technobass"
#include ".Xcolors/TomorrowNightEighties"
!#include ".Xcolors/Xtrail"
