# Defaults
# -------------------------------------------------------------

defaults
logfile ~/.maildir/msmtp.log

# me@rosstimson.com (iCloud)
# -------------------------------------------------------------

account rosstimson
auth on
host smtp.mail.me.com
port 465
protocol smtp
from me@rosstimson.com
user ross_timson@icloud.com
passwordeval op read op://personal/Apple/mu4e-isync-msmtp
tls on
tls_starttls off


# Set a default account
account default : rosstimson
