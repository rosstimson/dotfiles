.emacs.d/forge-database.sqlite
.emacs.d/history
.emacs.d/projects
.emacs.d/spell-fu
.emacs.d/straight/
.emacs.d/transient/
.emacs.d/url/

.gnupg/*
!.gnupg/gpg-agent.conf
!.gnupg/gpg.conf
!.gnupg/scdaemon.conf

.local/*
!.local/applications
!.local/rofi/themes

.vim/pack/minpac/start
